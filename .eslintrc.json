{
  "extends": ["airbnb-typescript/base"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "global-require": "off",
    "react/sort-comp": "off",
    "max-len": "off",
    "import/prefer-default-export": "off",
    "linebreak-style": "off"
  }
}
